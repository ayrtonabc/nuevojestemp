<svg width="500" height="300" viewBox="0 0 500 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="seoGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8f9fa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e9ecef;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="500" height="300" fill="url(#seoGradient)"/>
  
  <!-- SEO Dashboard mockup -->
  <rect x="50" y="50" width="400" height="200" fill="white" stroke="#dee2e6" stroke-width="2" rx="8"/>
  
  <!-- Header -->
  <rect x="50" y="50" width="400" height="40" fill="#FF3B30" rx="8 8 0 0"/>
  <text x="70" y="75" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white">
    Auditoría SEO Express
  </text>
  
  <!-- Content sections -->
  <rect x="70" y="110" width="120" height="60" fill="#e8f5e8" stroke="#28a745" stroke-width="1" rx="4"/>
  <text x="80" y="130" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#28a745">
    SEO Score
  </text>
  <text x="80" y="150" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#28a745">
    85/100
  </text>
  
  <rect x="210" y="110" width="120" height="60" fill="#fff3cd" stroke="#ffc107" stroke-width="1" rx="4"/>
  <text x="220" y="130" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#856404">
    Problemas
  </text>
  <text x="220" y="150" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#856404">
    12
  </text>
  
  <rect x="350" y="110" width="80" height="60" fill="#d1ecf1" stroke="#17a2b8" stroke-width="1" rx="4"/>
  <text x="360" y="130" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#0c5460">
    Páginas
  </text>
  <text x="360" y="150" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#0c5460">
    25+
  </text>
  
  <!-- Progress bars -->
  <rect x="70" y="190" width="360" height="8" fill="#e9ecef" rx="4"/>
  <rect x="70" y="190" width="306" height="8" fill="#28a745" rx="4"/>
  <text x="70" y="215" font-family="Arial, sans-serif" font-size="10" fill="#6c757d">
